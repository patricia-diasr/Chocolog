{
    "expo": {
        "name": "Chocolog",
        "slug": "Chocolog",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png",
            "name": "Chocolog",
            "shortName": "Chocolog",
            "display": "fullscreen",
            "backgroundColor": "#1f8f8f",
            "themeColor": "#1f8f8f"
        }
    }
}